<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view_controller.loginPageController">
   <children>
      <Label fx:id="userNameLabel" layoutX="53.0" layoutY="116.0" text="User Name" />
      <Label fx:id="passwordLabel" layoutX="53.0" layoutY="165.0" text="Password" />
      <TextField fx:id="userNameTextField" layoutX="149.0" layoutY="112.0" />
      <TextField fx:id="passwordTextField" layoutX="149.0" layoutY="161.0" />
      <Button fx:id="loginButton" layoutX="175.0" layoutY="246.0" mnemonicParsing="false" onAction="#OnLoginButtonClick" text="Log-In" />
      <Label fx:id="timeZoneLabel" layoutX="53.0" layoutY="212.0" text="Time-Zone" />
      <Label fx:id="zoneID" layoutX="149.0" layoutY="212.0" text="ZoneId" />
      <Label fx:id="loginTitleLabel" layoutX="106.0" layoutY="39.0" text="User Log-In">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="cancelButton" layoutX="246.0" layoutY="245.0" mnemonicParsing="false" onAction="#OnCancelButtonClick" text="Cancel" />
   </children>
</AnchorPane>
